datasource db {
  provider = "mysql"
}

generator client {
  provider = "prisma-client-js"
}

model Employee {
  id        Int      @id @default(autoincrement())
  nama      String
  nik       String   @unique
  nip       String   @unique
  jabatan   String
  alamat    String
  noTelp    String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

